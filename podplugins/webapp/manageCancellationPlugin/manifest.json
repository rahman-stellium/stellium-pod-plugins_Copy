{
  "sap.app": {
    "id": "stellium.ext.podplugins.manageCancellationPlugin",
    "type": "component",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "embeddedBy": "../"
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "stellium.ext.podplugins.manageCancellationPlugin.view.PluginView",
      "type": "XML",
      "id": "resourceListPlugin"
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "stellium.ext.podplugins.manageCancellationPlugin.i18n.i18n"
        }
      }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Targets",
        "viewType": "XML",
        "viewPath": "stellium.ext.podplugins.manageCancellationPlugin.view",
        "controlId": "idMainView",
        "controlAggregation": "content",
        "async": true
      },
      "targets": {
        "MasterList": {
          "viewName": "PluginView"
        },
        "OrderDetail": {
          "viewName": "PackingUnitDetails"
        }
      }
    }
  }
}
